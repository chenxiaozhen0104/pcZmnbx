<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Html.Partial("_Layout/_inc_header_res")
    @RenderSection("styles", required: false)
</head>
<body>
    <div>
        @Html.Partial("_Layout/head")
    </div>

    <div>
        @Html.Partial("_Layout_Sp/_inc_nav")
    </div>
    <div class="frame clearfix center-block">
        <div class="left">
            @Html.Partial("_Layout_Sp/_inc_menu")
        </div>
        <div class="main container">
            <div class="panel-title lineb">
                <h5>我是服务商 > @ViewBag.title1 > <span style="font-size: larger; font-weight: bold;color: red">@ViewBag.title2</span></h5>
            </div>
            @RenderBody()
        </div>
    </div>
    <div class="clearfix center-block">
        @Html.Partial("_Layout/bottom")
    </div>

    @Html.Partial("_Layout/_inc_dlgs")
    @Html.Partial("_Layout/_inc_bottom_res")
    @RenderSection("scripts", required: false) 
</body>
</html>
