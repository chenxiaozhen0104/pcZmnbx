<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Html.Partial("_Layout/_inc_header_res")
    @RenderSection("styles", required: false)

</head>
<body>
    <div>
        @Html.Partial("_Layout/head")
    </div>

    <div>
        @Html.Partial("_Layout_Baike/_inc_nav")
    </div>
    <div class="frame clearfix center-block">
        @RenderBody()
    </div>
    <div class="clearfix center-block">
        @Html.Partial("_Layout/bottom")
    </div>
    
    @Html.Partial("_Layout/_inc_dlgs")
    @Html.Partial("_Layout/_inc_bottom_res")
    @RenderSection("scripts", required: false)
</body>
</html>
